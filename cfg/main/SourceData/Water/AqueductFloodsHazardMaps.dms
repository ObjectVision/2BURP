	unit<uint8> OverlayRegios: nrofrows = 2
	{
		attribute<string> names: ['rcp4p5', 'rcp8p5'];
	}

	container OverlayGrids
	{
		attribute<uint2>  rcp4p5 (domain) := inun_rcp4p5_2080_rp1000_bool[uint2];
		attribute<uint2>  rcp8p5 (domain) := inun_rcp8p5_2080_rp1000_bool[uint2];
		
		attribute<string> Key     (domain) := string(rcp4p5) + "_" + string(rcp8p5);
		attribute<bool>   OnlyInrcp4p5 (domain) := strcount(key, '1_0') > 0;
		attribute<bool>   OnlyInrcp8p5 (domain) := strcount(key, '0_1') > 0;
		attribute<bool>   Bothrcp (domain)      := strcount(key, '1_1') > 0;
	}

	unit<uint32> rcpcombine:= overlay32(OverlayRegios/names, domain, OverlayGrids)
	{
		attribute<uint32> ID            := ID(.);
		attribute<string> rcp4p5_rcp8p5 := string(rcp4p5) + "_" + string(rcp8p5);
	}
