//GDP marker projections by the OECD: Rob Dellink, Jean Chateau, Elisa Lanzi, Bertrand Magné, Long-term economic growth projections in the Shared Socioeconomic Pathways, Global Environmental Change, Volume 42, 2017, Pages 200-214,
//ISSN 0959-3780, DOI:10.1016/j.gloenvcha.2015.06.004

	unit<uint32> SSP1
		: StorageName = "%projdir%/data/GDP/SSP1_OECDEnvGrowth_2005_USDB.csv"
		, StorageType = "gdal.vect"
		, StorageReadonly = "True"
		, DialogType  = "Map"
		, DialogData  = "PolyData"
	{
		attribute<Input/administrative/Countries/Country > Country_rel := rlookup(Region, Input/administrative/Countries/Country/ISO3);
		attribute<Input/LatLong> PolyData (polygon) := Input/administrative/Countries/Country/Geometry[Country_rel];
		attribute<string> Label := Region;

		attribute<BUSD> GDP_Y2010 := Y2010[BUSD];
		attribute<BUSD> GDP_Y2020 := Y2020[BUSD];
		attribute<BUSD> GDP_Y2030 := Y2030[BUSD];
		attribute<BUSD> GDP_Y2040 := Y2040[BUSD];
		attribute<BUSD> GDP_Y2050 := Y2050[BUSD];
		attribute<BUSD> GDP_Y2060 := Y2060[BUSD];
		attribute<BUSD> GDP_Y2070 := Y2070[BUSD];
		attribute<BUSD> GDP_Y2080 := Y2080[BUSD];
		attribute<BUSD> GDP_Y2090 := Y2090[BUSD];
		attribute<BUSD> GDP_Y2100 := Y2100[BUSD];
		
		attribute<kUSD> GDPCap_Y2010 := div(GDP_Y2010[kUSD],(rjoin(Region,/Claims/Scenario/M1/SSP1/FileData/Region,/Claims/Scenario/M1/SSP1/FileData/population_to_use_2010[float32])*1000000f));
		attribute<kUSD> GDPCap_Y2020 := div(GDP_Y2020[kUSD],(rjoin(Region,/Claims/Scenario/M1/SSP1/FileData/Region,/Claims/Scenario/M1/SSP1/FileData/population_to_use_2020[float32])*1000000f));
