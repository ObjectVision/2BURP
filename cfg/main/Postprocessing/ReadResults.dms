// Author:   Bas van Bemmel
// Function: results for now only to 2080 instead of 2100

container ReadResults: Using = "Units;"
{
	parameter<string> folder         :='LatLong_World';
	parameter<string> Method         := 'M3';
	parameter<string> Variant        := 'var1'; // Only relevant for M3, var1=JRC 50 percent bua, var2=GHSMOD/ Leave empty when working with M_1
	parameter<string> InPresentUrban := '';

	
	parameter<string> From   := 'All';
	parameter<string> Period := '';

	unit<uint32> SSP_Year := combine(Scenarios/SSP, Classifications/ProjectionDate)
	{
		attribute<string> SSP_c   := Scenarios/SSP/name[nr_1]+'/';
		attribute<string> SSP_c2  := Scenarios/SSP/name[nr_1];
		attribute<string> Year_c  := Classifications/ProjectionDate/name[nr_2];
		attribute<string> Year_c2 := string(Classifications/ProjectionDate/Yr[nr_2]);
		attribute<string> name    := SSP_c + Year_c;
	}

	#include <ResultVariables_Start.dms>
	
	container All: = ResultVariables/var(From,Period);
	#include <ResultVariables.dms>
	
	container AllGDP: = ResultVariablesGDP/var(From,Period);
	#include <ResultVariablesGDP.dms>
	#include <ReadStats.dms>
}